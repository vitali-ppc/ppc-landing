    <script>
      // WOW Tabs logic with fade and All
      const wowTabBtns = document.querySelectorAll('.wow-tab-btn');
      const wowTabPanes = document.querySelectorAll('.wow-tab-pane');
      wowTabBtns.forEach((btn) => {
        btn.addEventListener('click', function() {
          if (btn.classList.contains('active')) return;
          wowTabBtns.forEach(b => b.classList.remove('active'));
          btn.classList.add('active');
          const tab = btn.getAttribute('data-wowtab');
          wowTabPanes.forEach(pane => {
            if (pane.id === 'wowtab-' + tab) {
              pane.style.opacity = '1';
              pane.style.zIndex = '2';
              pane.style.pointerEvents = 'auto';
              pane.classList.add('active');
            } else {
              pane.style.opacity = '0';
              pane.style.zIndex = '1';
              pane.style.pointerEvents = 'none';
              pane.classList.remove('active');
            }
          });
          // Special for All: show all cards
          if(tab === 'all') {
            const allPane = document.getElementById('wowtab-all');
            allPane.style.opacity = '1';
            allPane.style.zIndex = '2';
            allPane.style.pointerEvents = 'auto';
            allPane.classList.add('active');
          }
        });
      });
      // Set All as default
      wowTabPanes.forEach(pane => {
        if(pane.id === 'wowtab-all') {
          pane.style.opacity = '1';
          pane.style.zIndex = '2';
          pane.style.pointerEvents = 'auto';
          pane.classList.add('active');
        } else {
          pane.style.opacity = '0';
          pane.style.zIndex = '1';
          pane.style.pointerEvents = 'none';
          pane.classList.remove('active');
        }
      });
    </script>
