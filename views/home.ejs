  <!-- === ВАРИАНТ 1: Digital Cosmos (живые звёзды/нейросеть) === -->
  <section style="background:#18191a;padding:64px 0 48px 0;overflow:hidden;position:relative;">
    <canvas id="cosmosCanvas" width="900" height="160" style="display:block;margin:0 auto;width:900px;height:160px;background:transparent;"></canvas>
    <script>
      // Живые звёзды и соединения (нейросеть)
      const canvas = document.getElementById('cosmosCanvas');
      const ctx = canvas.getContext('2d');
      const stars = Array.from({length:32},()=>({
        x:Math.random()*900,
        y:Math.random()*160,
        r:2+Math.random()*2,
        dx:(Math.random()-0.5)*0.2,
        dy:(Math.random()-0.5)*0.2
      }));
      function drawCosmos(){
        ctx.clearRect(0,0,900,160);
        // Соединения
        for(let i=0;i<stars.length;i++){
          for(let j=i+1;j<stars.length;j++){
            const dx=stars[i].x-stars[j].x,dy=stars[i].y-stars[j].y,dist=Math.sqrt(dx*dx+dy*dy);
            if(dist<120){
              ctx.strokeStyle='rgba(127,156,245,0.13)';ctx.lineWidth=1;
              ctx.beginPath();ctx.moveTo(stars[i].x,stars[i].y);ctx.lineTo(stars[j].x,stars[j].y);ctx.stroke();
            }
          }
        }
        // Звёзды
        for(const s of stars){
          ctx.beginPath();ctx.arc(s.x,s.y,s.r,0,2*Math.PI);
          ctx.fillStyle='#7f9cf5';ctx.globalAlpha=0.7+0.3*Math.sin(Date.now()/700+s.x);
          ctx.fill();ctx.globalAlpha=1;
        }
        // Движение
        for(const s of stars){
          s.x+=s.dx;s.y+=s.dy;
          if(s.x<0||s.x>900) s.dx*=-1;
          if(s.y<0||s.y>160) s.dy*=-1;
        }
        requestAnimationFrame(drawCosmos);
      }
      drawCosmos();
    </script>
  </section>

  <!-- === ВАРИАНТ 2: Digital Cosmos (градиент + glow) === -->
  <section style="background:#18191a;padding:64px 0 48px 0;overflow:hidden;position:relative;">
    <canvas id="cosmosCanvasGlow" width="900" height="160" style="display:block;margin:0 auto;width:900px;height:160px;background:transparent;"></canvas>
    <script>
      // Живые звёзды с градиентом и glow
      const canvasGlow = document.getElementById('cosmosCanvasGlow');
      const ctxGlow = canvasGlow.getContext('2d');
      const starsGlow = Array.from({length:32},()=>({
        x:Math.random()*900,
        y:Math.random()*160,
        r:2+Math.random()*2,
        dx:(Math.random()-0.5)*0.2,
        dy:(Math.random()-0.5)*0.2
      }));
      function drawCosmosGlow(){
        ctxGlow.clearRect(0,0,900,160);
        // Соединения
        for(let i=0;i<starsGlow.length;i++){
          for(let j=i+1;j<starsGlow.length;j++){
            const dx=starsGlow[i].x-starsGlow[j].x,dy=starsGlow[i].y-starsGlow[j].y,dist=Math.sqrt(dx*dx+dy*dy);
            if(dist<120){
              ctxGlow.save();
              const grad=ctxGlow.createLinearGradient(starsGlow[i].x,starsGlow[i].y,starsGlow[j].x,starsGlow[j].y);
              grad.addColorStop(0,'#7f9cf5');
              grad.addColorStop(1,'#00ffe7');
              ctxGlow.strokeStyle=grad;ctxGlow.globalAlpha=0.13;
              ctxGlow.lineWidth=1;
              ctxGlow.beginPath();ctxGlow.moveTo(starsGlow[i].x,starsGlow[i].y);ctxGlow.lineTo(starsGlow[j].x,starsGlow[j].y);ctxGlow.stroke();
              ctxGlow.restore();
            }
          }
        }
        // Звёзды
        for(const s of starsGlow){
          ctxGlow.save();
          const grad=ctxGlow.createRadialGradient(s.x,s.y,0,s.x,s.y,s.r*3.5);
          grad.addColorStop(0,'#00ffe7');
          grad.addColorStop(0.5,'#7f9cf5');
          grad.addColorStop(1,'rgba(0,255,231,0)');
          ctxGlow.beginPath();ctxGlow.arc(s.x,s.y,s.r*3.5,0,2*Math.PI);
          ctxGlow.globalAlpha=0.25;
          ctxGlow.fillStyle=grad;ctxGlow.fill();ctxGlow.restore();
          ctxGlow.beginPath();ctxGlow.arc(s.x,s.y,s.r,0,2*Math.PI);
          ctxGlow.globalAlpha=0.9;
          ctxGlow.fillStyle='#7f9cf5';ctxGlow.shadowColor='#00ffe7';ctxGlow.shadowBlur=8;
          ctxGlow.fill();ctxGlow.shadowBlur=0;ctxGlow.globalAlpha=1;
        }
        // Движение
        for(const s of starsGlow){
          s.x+=s.dx;s.y+=s.dy;
          if(s.x<0||s.x>900) s.dx*=-1;
          if(s.y<0||s.y>160) s.dy*=-1;
        }
        requestAnimationFrame(drawCosmosGlow);
      }
      drawCosmosGlow();
    </script>
  </section>

